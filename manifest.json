{
  "manifest_version" : 2,
  "name" : "Checklist",
  "description" : "Checklist for things you want to do, reminder and share",
  "version" : "1.0",
  "browser_action" : {
    "default_icon" : "icon.png",
    "default_popup" : "checklist.html",
    "default_title": "Add Checklist"
  },
  "background": {
    "scripts": ["checklist.js"]
  },
  "permissions" : [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
